server:
  port: 8080

spring:
  jpa:
    database: POSTGRESQL
    show-sql: true
    open-in-view: true
    properties:
      dialect: org.hibernate.dialect.PostgreSQLDialect
      hibernate.temp.use_jdbc_metadata_defaults: false
  servlet:
    multipart:
      max-file-size: 20MB
      max-request-size: 20MB

surwave:
  authType: basic
  uploadDir: data
