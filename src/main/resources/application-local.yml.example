server:
  port: 8080

spring:
  jpa:
    database: POSTGRESQL
    show-sql: true
    open-in-view: true
    hibernate:
      ddl-auto: create-drop
    properties:
      dialect: org.hibernate.dialect.PostgreSQLDialect
      hibernate.temp.use_jdbc_metadata_defaults: false

  datasource:
    platform: postgres
    url: jdbc:postgresql://localhost:5432/surwave
    username: postgres
    password: postgres
    driverClassName: org.postgresql.Driver
    initialize: false

surwave:
  authType: basic
